{% assign webpack_build_timestamp = "<%= (new Date()).getTime() %>" %}
{% assign webpack_chunks = "<%= Object.keys(htmlWebpackPlugin.files.chunks) %>" | split: "," %}
<% for (var chunk in htmlWebpackPlugin.files.chunks) { %>
{% set webpack_js, "<%= chunk %>", "<%= htmlWebpackPlugin.files.chunks[chunk].entry.replace('/javascripts/', '') %>" %}
{% assign temp = "<%= htmlWebpackPlugin.files.chunks[chunk].css.map((e) => e.replace('/stylesheets/', '')) %>" | split: "," %}
{% set webpack_css, "<%= chunk %>", temp %}
<% } %>
