{
  "name": "nimbu-toolbelt",
  "description": "Tools for Nimbu projects",
  "version": "1.2.0",
  "author": "Zenjoy Engineering",
  "bin": {
    "nimbu": "./bin/run"
  },
  "bugs": "https://github.com/zenjoy/nimbu-toolbelt/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.116",
    "@types/node-fetch": "^2.1.2",
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-root-import": "^6.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.1",
    "cli-ux": "^4.8.2",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.1.1",
    "css-loader": "^1.0.0",
    "csso-webpack-plugin": "^1.0.0-beta.12",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.11",
    "node-fetch": "^2.2.0",
    "node-sass": "^4.9.3",
    "node-yaml": "^3.1.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "promise": "^8.0.2",
    "react-dev-utils": "^5.0.2",
    "react-svg-core": "^2.1.0",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.2",
    "style-loader": "^0.23.0",
    "tslib": "^1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "urlencode": "^1.1.0",
    "webpack": "^4.19.1",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/chalk": "^2.2.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/webpack": "^4.4.12",
    "@types/webpack-dev-server": "^3.1.1",
    "globby": "^8",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json",
    "/Gemfile",
    "/Gemfile.lock",
    "/polyfills.js",
    "/template",
    "/README.md"
  ],
  "homepage": "https://github.com/zenjoy/nimbu-toolbelt",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nimbu",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "auth": {
        "description": "Authenticate, display token and current user"
      },
      "init": {
        "description": "working directory initialization"
      },
      "sites": {
        "description": "interacting with your sites (list, create)"
      },
      "themes": {
        "description": "working with themes (upload / download)"
      },
      "browse": {
        "description": "open the current site in your browser (simulator, admin)"
      },
      "apps": {
        "description": "manage (cloud code) applications"
      }
    }
  },
  "repository": "zenjoy/nimbu-toolbelt",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "tsc && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "postinstall": "bundle install"
  },
  "types": "lib/index.d.ts"
}
