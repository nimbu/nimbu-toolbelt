{
  "name": "nimbu-toolbelt",
  "description": "Tools for Nimbu projects",
  "version": "1.2.5",
  "author": "Zenjoy Engineering",
  "bin": {
    "nimbu": "./bin/run"
  },
  "bugs": "https://github.com/zenjoy/nimbu-toolbelt/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-autocomplete": "^0.1.0",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-not-found": "^1.2.2",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.116",
    "@types/node-fetch": "^2.1.2",
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-root-import": "^6.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.1",
    "cli-ux": "^5.2.0",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.1.1",
    "css-loader": "^1.0.0",
    "csso-webpack-plugin": "^1.0.0-beta.12",
    "date-fns": "^2.0.0-alpha.27",
    "debug": "^4.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^7.0.0",
    "fuzzy": "^0.1.3",
    "glob": "^7.1.3",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "hyperlinker": "^1.0.0",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.11",
    "log-symbols": "^2.2.0",
    "netrc-parser": "^3.1.6",
    "nimbu-client": "^1.0.0",
    "node-fetch": "^2.2.0",
    "node-sass": "^4.9.3",
    "node-yaml": "^3.1.1",
    "os": "^0.1.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "promise": "^8.0.2",
    "react-dev-utils": "^5.0.2",
    "react-svg-core": "^2.1.0",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.2",
    "style-loader": "^0.23.0",
    "supports-color": "^6.1.0",
    "supports-hyperlinks": "^1.0.1",
    "tslib": "^1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "urlencode": "^1.1.0",
    "webpack": "^4.19.1",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/chalk": "^2.2.0",
    "@types/mocha": "^5",
    "@types/node": "^10.12.27",
    "@types/webpack": "^4.4.12",
    "@types/webpack-dev-server": "^3.1.1",
    "ava": "^1.2.1",
    "chai": "^4.2.0",
    "codecov": "^3.2.0",
    "fancy-test": "^1.4.3",
    "globby": "^8",
    "mocha": "^6.0.2",
    "mock-fs": "^4.8.0",
    "nock": "^10.0.6",
    "nyc": "^13.3.0",
    "ts-node": "^7",
    "tslint": "^5",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json",
    "/Gemfile",
    "/Gemfile.lock",
    "/polyfills.js",
    "/template",
    "/README.md"
  ],
  "homepage": "https://github.com/zenjoy/nimbu-toolbelt",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nimbu",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete"
    ],
    "topics": {
      "auth": {
        "description": "Authenticate, display token and current user"
      },
      "init": {
        "description": "working directory initialization"
      },
      "sites": {
        "description": "interacting with your sites (list, create)"
      },
      "themes": {
        "description": "working with themes (upload / download)"
      },
      "browse": {
        "description": "open the current site in your browser (simulator, admin)"
      },
      "apps": {
        "description": "manage (cloud code) applications"
      }
    }
  },
  "repository": "zenjoy/nimbu-toolbelt",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc ava --verbose",
    "watch": "ava --verbose --watch",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "sources": [
      "**/*.{js,ts}",
      "!lib/**/*"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
