{
  "name": "@nimbu-cli/proxy-server",
  "version": "1.0.0",
  "description": "HTTP proxy server for Nimbu CLI development",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib node_modules/.cache && rm -f tsconfig.tsbuildinfo && tsc -b",
    "clean": "rimraf lib",
    "prepack": "yarn build",
    "test": "mocha --require ts-node/register 'test/**/*.test.ts'",
    "lint": "eslint src --ext .ts",
    "yalc:publish": "yarn build && yalc publish --push --no-script"
  },
  "dependencies": {
    "@nimbu-cli/command": "^5.5.0",
    "axios": "^1.10.0",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/chai": "^4",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10",
    "@types/multer": "^1.4.7",
    "@types/node": "^24.0.3",
    "chai": "^4",
    "mocha": "^10",
    "rimraf": "^3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "license": "MIT"
}
