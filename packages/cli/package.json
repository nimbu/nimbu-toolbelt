{
  "name": "nimbu",
  "description": "Tools for Nimbu projects",
  "version": "5.0.0",
  "author": "Zenjoy Engineering",
  "bin": {
    "nimbu": "./bin/run"
  },
  "bugs": "https://github.com/zenjoy/nimbu-toolbelt/issues",
  "dependencies": {
    "@nimbu-cli/command": "^1",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "deep-object-diff": "^1.1.0",
    "front-matter": "^3.0.2",
    "date-fns": "^2",
    "fuzzy": "^0.1.3",
    "hyperlinker": "^1",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "js-yaml": "^3.13.1",
    "listr": "^0.14.3",
    "lodash": "^4.17.11",
    "lodash.clonedeep": "^4.5.0",
    "log-symbols": "^3.0.0",
    "pretty-bytes": "^5.3.0",
    "rxjs": "^6.5.2",
    "supports-hyperlinks": "^1.0.1",
    "tmp-promise": "^2.0.2",
    "through": "^2.3.8"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json",
    "/polyfills.js",
    "/README.md"
  ],
  "homepage": "https://github.com/zenjoy/nimbu-toolbelt",
  "keywords": [
    "oclif",
    "nimbu",
    "nimbu-cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nimbu",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete"
    ],
    "topics": {
      "auth": {
        "description": "authenticate, display token and current user"
      },
      "init": {
        "description": "working directory initialization"
      },
      "sites": {
        "description": "interacting with your sites (list, create)"
      },
      "themes": {
        "description": "working with themes (upload / download)"
      },
      "browse": {
        "description": "open the current site in your browser (simulator, admin)"
      },
      "apps": {
        "description": "manage (cloud code) applications"
      },
      "mails": {
        "description": "manage your notification templates"
      },
      "channels": {
        "description": "working with channels"
      },
      "products": {
        "description": "working with products"
      },
      "customers": {
        "description": "working with customers"
      },
      "notifications": {
        "description": "working with notifications"
      },
      "translations": {
        "description": "working with translations"
      }
    }
  },
  "repository": "zenjoy/nimbu-toolbelt",
  "scripts": {
    "build": "rm -rf lib && rm -f tsconfig.tsbuildinfo && tsc -b && oclif-dev manifest && oclif-dev readme",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "tsc && oclif-dev manifest && oclif-dev readme",
    "test": "TS_NODE_FILES=false TS_NODE_TRANSPILE_ONLY=true mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "mocha": {
    "diff": true,
    "recursive": true,
    "extension": [
      "ts"
    ],
    "opts": "./test/mocha.opts",
    "package": "./package.json",
    "reporter": "spec",
    "slow": 75,
    "timeout": 5000,
    "ui": "bdd"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
