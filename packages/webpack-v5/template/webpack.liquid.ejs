{% assign <%= prefix %>webpack_build_timestamp = "<%= (new Date()).getTime() %>" %}

<% const js_files = htmlWebpackPlugin.files.js.map((f) => f.replace('/javascripts/', '')) %>
{% assign <%= prefix %>webpack_js = '<%= JSON.stringify(js_files) %>' | from_json %}

<% const css_files = htmlWebpackPlugin.files.css.map((f) => f.replace('/stylesheets/', '')) %>
{% assign <%= prefix %>webpack_css = '<%= JSON.stringify(css_files) %>' | from_json %}
